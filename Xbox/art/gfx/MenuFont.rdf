// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         MenuFont.h
out_packedresource MenuFont.xpr
out_error          MenuFont.err


Texture Font
{
   Source      MenuFont.bmp
   AlphaSource MenuFont_a.bmp
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile    MenuFont.abc
}

