// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         GameFont.h
out_packedresource GameFont.xpr
out_error          GameFont.err


Texture Font
{
   Source      GameFont.bmp
   AlphaSource GameFont_a.bmp
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile    GameFont.abc
}

