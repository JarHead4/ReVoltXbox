// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         BigFont.h
out_packedresource BigFont.xpr
out_error          BigFont.err


Texture Font
{
   Source      BigFont.bmp
   AlphaSource BigFont_a.bmp
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile BigFont.abc
}

